  entidad.telefono, 
  entidad.razonsocial,
  provincia.descripcion as provincia,
  distrito.descripcion as distrito,
  departamento.descripcion as departamento
FROM 
  public.detallenotaingreso, 
  public.notaingreso, 
  public.almacen, 
  public.producto, 
  public.unidadmedida, 
  public.estadoni, 
  public.entidad, 
  public.departamento, 
  public.distrito, 
  public.provincia
WHERE 
  notaingreso.idnotaingreso = detallenotaingreso.idnotaingreso AND
  almacen.idalmacen = notaingreso.idalmacen AND
  almacen.iddistrito = distrito.iddistrito AND
  almacen.idprovincia = distrito.idprovincia AND
  almacen.iddepartamento = distrito.iddepartamento AND
  producto.idproducto = detallenotaingreso.idproducto AND
  unidadmedida.idunidadmedida = producto.idunidadmedida AND
  estadoni.idestadoni = notaingreso.idestadoni AND
  entidad.identidad = notaingreso.identidad AND
  distrito.idprovincia = provincia.idprovincia AND
  distrito.iddepartamento = provincia.iddepartamento AND
  provincia.iddepartamento = departamento.iddepartamento AND
  notaingreso.idnotaingreso = $P{Orden};]]>
